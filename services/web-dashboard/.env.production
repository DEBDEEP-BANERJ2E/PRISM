# Production Environment Variables for Web Dashboard
NODE_ENV=production
PORT=10000

# API Configuration
VITE_API_BASE_URL=https://your-render-domain.onrender.com
VITE_NOTIFICATION_SERVICE_URL=https://your-notification-service.onrender.com
VITE_ML_BACKEND_URL=https://your-ml-backend.onrender.com
VITE_SUPABASE_BACKEND_URL=https://your-supabase-backend.onrender.com

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# CORS Configuration
ALLOWED_ORIGINS=https://your-render-domain.onrender.com,https://your-custom-domain.com

# Mapbox Configuration (for 3D visualization)
VITE_MAPBOX_ACCESS_TOKEN=your-mapbox-token

# Logging
LOG_LEVEL=info