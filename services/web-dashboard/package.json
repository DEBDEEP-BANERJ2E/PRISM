{
  "name": "prism-web-dashboard",
  "version": "1.0.0",
  "description": "PRISM Web Dashboard - 3D Interactive Risk Visualization Platform",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "vite",
    "dev:server": "ts-node-dev --respawn --transpile-only server/index.ts",
    "dev:full": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "build": "vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "node dist/server/index.js",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "vercel-build": "npm run build",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.3",
    "@mui/material": "^5.15.3",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.15.12",
    "@supabase/supabase-js": "^2.57.4",
    "@types/dompurify": "^3.0.5",
    "@types/papaparse": "^5.3.16",
    "@types/react-dropzone": "^4.2.2",
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dompurify": "^3.2.7",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "framer-motion": "^10.16.16",
    "gsap": "^3.13.0",
    "helmet": "^7.1.0",
    "lucide-react": "^0.544.0",
    "mapbox-gl": "^2.15.0",
    "multer": "^1.4.5-lts.1",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-map-gl": "^7.0.21",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.8.0",
    "recharts": "^2.8.0",
    "socket.io-client": "^4.7.4",
    "three": "^0.159.0",
    "@mui/x-data-grid": "^6.19.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/form-data": "^2.2.1",
    "@types/mapbox-gl": "^2.7.19",
    "@types/multer": "^1.4.11",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/three": "^0.158.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^27.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  }
}
